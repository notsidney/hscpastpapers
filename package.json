{
  "name": "hscpastpapers",
  "version": "2.0.0",
  "author": "Sidney Alcantara",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/notsidney/hscpastpapers.git"
  },
  "source": "src/index.html",
  "browserslist": "> 0.5%, not dead, not op_mini all, not ie > 0, not and_uc > 0",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "prepare": "husky install"
  },
  "dependencies": {
    "@iconicicons/react": "^1.5.1",
    "axios": "^1.2.0",
    "react": "^16",
    "react-dom": "^16"
  },
  "devDependencies": {
    "@types/react": "^16",
    "@types/react-dom": "^16",
    "@vitejs/plugin-react": "^2.2.0",
    "husky": "^8.0.2",
    "lint-staged": "^13.0.3",
    "postcss-preset-env": "^7.8.3",
    "prettier": "2.8.0",
    "vite": "^3.2.7"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "postcss": {
    "plugins": {
      "postcss-preset-env": {
        "features": {
          "oklab-function": {
            "subFeatures": {
              "displayP3": false
            }
          }
        }
      }
    }
  }
}
